pipeline {
   agent any
   stages {
       stage('1-Build Code') {
           steps {
               echo "Building Artifact"
           }
       }
      stage('2-Deploy Code') {
          steps {
               echo "Deploying Code"
               echo "testing"
          }
      }
      stage(3-testing code){
        steps{
            sh 'lscp'
        }
      }
   }
}
